{
  "name": "kiwiii",
  "version": "0.8.3",
  "author": "Seiji Matsuoka",
  "homepage": "https://github.com/mojaie/kiwiii",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/mojaie/kiwiii.git"
  },
  "description": "Kinetic interactive web interface for intuitive informatics",
  "license": "MIT",
  "main": "main.js",
  "devDependencies": {
    "argv": "0.0.2",
    "d3": "^4.12.0",
    "ejs": "^2.5.7",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.5.1",
    "less": "^2.7.3",
    "rollup": "^0.52.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sw-precache": "^5.2.0",
    "sw-toolbox": "^3.6.0",
    "tape": "^4.8.0",
    "uglify-es": "^3.2.0"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "tape 'test/**/*-test.js'",
    "docs": "mkdir -p '_build/docs' && jsdoc -r src -d '_build/docs'",
    "build": "node rollup.js '_build' --debug=true",
    "deploy": "npm run docs && node rollup.js 'dist' --debug=false && node rollup.js '_build' --debug=false"
  }
