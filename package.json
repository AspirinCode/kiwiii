  "main": "main.js",
  "devDependencies": {
    "argv": "0.0.2",
    "d3": "^5.0.0",
    "ejs": "^2.5.8",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.6.2",
    "less": "^3.0.1",
    "rollup": "^0.57.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sw-precache": "^5.2.1",
    "sw-toolbox": "^3.6.0",
    "tape": "^4.9.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "tape 'test/**/*-test.js'",
